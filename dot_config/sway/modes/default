$unbindsym $mod+Return
$unbindsym $mod+Shift+Return
# $bindsym $mod+Return exec $term
$bindsym $mod+Return exec 'kitty --listen-on=unix:@"$(date +%s%N)"'
$bindsym $mod+Shift+Return exec $browser

# reserve mod+{,Shift}+p for clipboard manipulations 
# and move swayr to mod+Shift+w
$unbindsym $mod+p
$unbindsym $mod+Shift+p
$bindsym $mod+p exec $clipboard
$bindsym $mod+Shift+p exec $clipboard-del
$bindsym $mod+Shift+w exec env RUST_BACKTRACE=1 swayr switch-window &>> /tmp/swayr.log
